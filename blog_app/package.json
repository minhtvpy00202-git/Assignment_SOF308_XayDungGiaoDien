{
  "name": "blog_app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:server\" --names \"VITE,JSON\" --prefix-colors \"cyan,yellow\"",
    "dev:vite": "vite",
    "dev:server": "json-server --watch db.json --port 3000",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "server": "json-server --watch db.json --port 3000",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "vue": "^3.5.24",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^4.0.14",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "fast-check": "^4.3.0",
    "jsdom": "^27.2.0",
    "json-server": "^1.0.0-beta.3",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vitest": "^4.0.14",
    "vue-tsc": "^3.1.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
